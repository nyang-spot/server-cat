generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["interactiveTransactions"]
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Users {
    id       Int     @id @default(autoincrement())
    name      String? @db.VarChar(255)

    @@map("users")
}